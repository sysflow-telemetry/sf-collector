@namespace("sysflow.flow")
protocol SysFlow {
import idl "../type/datatypes.avdl";
record NetworkFlow {
  sysflow.type.OID procOID; // host ID + monotonic process creation time + host PID
  timestamp_ms ts;
  long tid;
  int opFlags;
  timestamp_ms endTs;
  int sip;
  int sport;
  int dip;
  int dport;
  int proto;
  int fd;
  long numRRecvOps;
  long numWSendOps;
  long numRRecvBytes;
  long numWSendBytes;
}
}
